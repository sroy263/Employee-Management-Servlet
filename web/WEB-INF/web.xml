<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>SupermarketModel.Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/SupermarketModel/Login</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>menu</servlet-name>
        <servlet-class>SupermarketModel.Menu</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>menu</servlet-name>
        <url-pattern>/SupermarketModel/Menu</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>registration</servlet-name>
        <servlet-class>SupermarketModel.Registration</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>registration</servlet-name>
        <url-pattern>/SupermarketModel/Registration</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>registrationreport</servlet-name>
        <servlet-class>SupermarketModel.RegistrationReport</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>registrationreport</servlet-name>
        <url-pattern>/SupermarketModel/RegistrationReport</url-pattern>
    </servlet-mapping>
    
    
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>SupermarketModel.Logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/SupermarketModel/Logout</url-pattern>
    </servlet-mapping>
    
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
